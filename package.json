  "lint-staged": {
    "*.{js,ts,jsx,tsx,json}": ["biome check --apply"]
  },
  "dependencies": {
    "@react-three/drei": "^10.0.6",
    "@react-three/fiber": "^9.1.0",
    "@tailwindcss/vite": "^4.1.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "^4.1.5",
    "three": "^0.176.0",
    "use-error-boundary": "^2.0.6"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.1.3",
    "@types/three": "^0.176.0",
    "@vitejs/plugin-react": "^4.4.1",
    "gh-pages": "^6.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "typescript": "~5.7.2",
    "vite": "^6.3.5"
  }
